    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "husky": ">=6",
    "jest": "^27.5.1",
    "lint-staged": ">=10",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tsup": "^5.12.6",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
